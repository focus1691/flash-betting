{
  "name": "flash-betting",
  "author": "Joshua",
  "description": "Bet Trading on the BetFair Exchange",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.11.2",
    "@stripe/react-stripe-js": "^1.2.2",
    "@stripe/stripe-js": "^1.12.1",
    "clsx": "^1.1.1",
    "color.js": "^0.1.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "electron-context-menu": "^0.15.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "html-react-parser": "^0.14.0",
    "immutability-helper": "^3.1.1",
    "jquery": "^3.5.1",
    "lightweight-charts": "^3.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-flag-icon-css": "^1.0.25",
    "react-helmet": "^5.2.1",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-useinterval": "^1.0.2",
    "react-uuid": "^1.0.2",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io": "^2.3.0",
    "sqlite-async": "^1.1.1",
    "universal-cookie": "^4.0.4",
    "url-regex": "^5.0.0",
    "validator": "^13.1.17"
  },
  "homepage": "./",
  "build": {
    "appId": "com.flashbetting.app",
    "files": [
      "./build/**/*",
      "./server/**/*",
      "./public/electron.js",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "publisherName": "Flash Betting",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "target": "dmg"
    }
  },
  "scripts": {
    "start": "cross-env PORT=9090 react-scripts start",
    "build": "react-scripts build",
    "rebuild": "electron-rebuild -f -w node_modules/sqlite3",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cross-env nodemon server/index.js",
    "dev": "cross-env concurrently \"npm run start\" \"npm run server\"",
    "app": "cross-env BROWSER=none npm run build && electron .",
    "electron-build": "cross-env npm run build && electron-builder"
  },
  "main": "public/electron.js",
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "express-pino-logger": "^4.0.0",
    "jest": "^26.6.1",
    "local-ssl-proxy": "^1.3.0",
    "node-env-run": "^3.0.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^1.6.1",
    "webpack": "^4.44.2"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
